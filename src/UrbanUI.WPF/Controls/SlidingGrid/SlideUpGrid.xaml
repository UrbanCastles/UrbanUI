<UserControl x:Class="UrbanUI.WPF.Controls.SlideUpGrid"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:UrbanUI.WPF.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">


    <UserControl.Template>
        <ControlTemplate TargetType="UserControl">
            <Grid Name="contentGrid">
                <local:ContentPresenter x:Name="contentPresenter"  Content="{TemplateBinding Content}"  ContentChanged="contentPresenter_ContentChanged"/>
            </Grid>
        </ControlTemplate>
    </UserControl.Template>
</UserControl>
    
    
    
    
